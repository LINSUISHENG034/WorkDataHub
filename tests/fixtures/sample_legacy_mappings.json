  "test_cases": {
    "priority_resolution": [
      {
        "name": "plan_takes_priority_over_name",
        "query": {
          "plan_code": "AN001",
          "customer_name": "中国平安保险股份有限公司"
        },
        "expected": {
          "company_id": "614810477",
          "match_type": "plan",
          "source_value": "AN001",
          "priority": 1
        }
      },
      {
        "name": "account_takes_priority_over_hardcode",
        "query": {
          "account_number": "123456789",
          "plan_code": "FP0001"
        },
        "expected": {
          "company_id": "614810477",
          "match_type": "account",
          "source_value": "123456789",
          "priority": 2
        }
      },
      {
        "name": "hardcode_uses_plan_code_as_key",
        "query": {
          "plan_code": "FP0001",
          "customer_name": "some name"
        },
        "expected": {
          "company_id": "614810477",
          "match_type": "hardcode",
          "source_value": "FP0001",
          "priority": 3
        }
      },
      {
        "name": "customer_name_fallback",
        "query": {
          "customer_name": "中国平安保险股份有限公司"
        },
        "expected": {
          "company_id": "614810477",
          "match_type": "name",
          "source_value": "中国平安保险股份有限公司",
          "priority": 4
        }
      },
      {
        "name": "account_name_last_priority",
        "query": {
          "account_name": "平安保险年金计划账户",
          "customer_name": "has value to prevent default"
        },
        "expected": {
          "company_id": "614810477",
          "match_type": "account_name",
          "source_value": "平安保险年金计划账户",
          "priority": 5
        }
      }
    ],
    "default_fallback": [
      {
        "name": "null_customer_name_triggers_default",
        "query": {
          "customer_name": null
        },
        "expected": {
          "company_id": "600866980",
          "match_type": "default",
          "source_value": null,
          "priority": null
        }
      },
      {
        "name": "empty_customer_name_triggers_default",
        "query": {
          "customer_name": ""
        },
        "expected": {
          "company_id": "600866980",
          "match_type": "default",
          "source_value": null,
          "priority": null
        }
      },
      {
        "name": "whitespace_customer_name_triggers_default",
        "query": {
          "customer_name": "   "
        },
        "expected": {
          "company_id": "600866980",
          "match_type": "default",
          "source_value": null,
          "priority": null
        }
      }
    ],
    "chinese_character_edge_cases": [
      {
        "name": "exact_chinese_match",
        "query": {
          "customer_name": "中国平安保险股份有限公司"
        },
        "expected": {
          "company_id": "614810477",
          "match_type": "name",
          "source_value": "中国平安保险股份有限公司"
        }
      },
      {
        "name": "chinese_account_name_match",
        "query": {
          "account_name": "平安保险年金计划账户",
          "customer_name": "not empty"
        },
        "expected": {
          "company_id": "614810477",
          "match_type": "account_name",
          "source_value": "平安保险年金计划账户"
        }
      },
      {
        "name": "chinese_no_match",
        "query": {
          "customer_name": "不存在的公司名称"
        },
        "expected": {
          "company_id": null,
          "match_type": null,
          "source_value": null
        }
      }
    ],
    "null_empty_handling": [
      {
        "name": "all_nulls",
        "query": {
          "plan_code": null,
          "account_number": null,
          "customer_name": null,
          "account_name": null
        },
        "expected": {
          "company_id": "600866980",
          "match_type": "default"
        }
      },
      {
        "name": "all_empty_strings",
        "query": {
          "plan_code": "",
          "account_number": "",
          "customer_name": "",
          "account_name": ""
        },
        "expected": {
          "company_id": "600866980",
          "match_type": "default"
        }
      },
      {
        "name": "mixed_null_empty",
        "query": {
          "plan_code": null,
          "account_number": "",
          "customer_name": "   ",
          "account_name": null
        },
        "expected": {
          "company_id": "600866980",
          "match_type": "default"
        }
      }
    ],
    "case_sensitivity": [
      {
        "name": "exact_case_match",
        "query": {
          "plan_code": "AN001"
        },
        "expected": {
          "company_id": "614810477",
          "match_type": "plan"
        }
      },
      {
        "name": "wrong_case_no_match",
        "query": {
          "plan_code": "an001"
        },
        "expected": {
          "company_id": "600866980",
          "match_type": "default"
        }
      }
    ]
